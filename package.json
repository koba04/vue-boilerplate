{
  "name": "my-vuejs-boilerplate",
  "version": "0.0.1",
  "description": "vue.js sample project",
  "repository": "https://github.com/koba04/vue-boilerplate",
  "main": "src/app.coffee",
  "dependencies": {
    "vue": "~0.10.3",
    "superagent": "~0.17.0",
    "director": "~1.2.3"
  },
  "devDependencies": {
    "browserify": "~3.41.0",
    "partialify": "~3.0.0",
    "insert-css": "~0.1.1",
    "beefy": "~1.1.0",
    "coffeeify": "~0.6.0",
    "stylify": "~0.1.3"
  },
  "author": "koba04",
  "license": "MIT",
  "scripts": {
    "dev": "beefy src/index.coffee:bundle.js --live -- --extension=coffee",
    "build": "browserify src/index.coffee --extension=coffee > bundle.js"
  },
  "browserify": {
    "transform": [
      ["partialify"],
      ["coffeeify"],
      ["stylify"]
    ]
  }
}
